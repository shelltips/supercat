{
  "name": "supercat",
  "version": "0.1.0",
  "description": "My node module",
  "main": "dist/index.js",
  "scripts": {
    "test": "istanbul cover _mocha -- --compilers js:babel/register",
    "test:coveralls": "istanbul cover _mocha --report lcovonly -- --compilers js:babel/register",
    "mocha": "mocha test --compilers js:babel/register",
    "build": "rm -rf dist/* && npm run copy:others && babel source --out-dir dist --presets es2015",
    "copy:others" : "rsync -avm --exclude='*.js' ./source/* ./dist",
    "see-output" : "babel-node source/cli.js ../../../src/pushapp-backend/config/deploy.rb --presets es2015"
  },
  "repository": "sohamkamani/supercat",
  "author": {
    "name": "sohamkamani",
    "email": "sohamkamani@gmail.com"
  },
  "bin": {
    "scat": "./dist/cli.js"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "colors": "^1.1.2",
    "cpy-cli": "^1.0.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.1.0",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "color": "^0.10.1",
    "css": "^2.2.1",
    "first-mate": "^5.1.1",
    "first-mate-select-grammar": "^1.0.1",
    "highlights": "^1.3.1",
    "htmlout": "^0.2.0",
    "lodash": "^3.10.1",
    "parse-color": "^1.0.0"
  }
}
